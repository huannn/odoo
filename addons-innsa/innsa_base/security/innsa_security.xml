<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">
    <record id="innsa_category" model="ir.module.category">
        <field name="name">Innsa.</field>
        <field name="description">Innsa Access</field>
        <field name="sequence">50</field>
    </record>

    <record id="group_innsa_admin" model="res.groups">
        <field name="name">Innsa Admin</field>
        <field name="category_id" ref="innsa_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>
